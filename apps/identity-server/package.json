{
  "name": "@yekalaitech/identity-server",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "npm run lint && rm -rf .svelte-kit && svelte-kit build && nest build -p tsconfig-server.json",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "svelte-kit dev",
    "fix:lint": "eslint src --ext .ts --fix",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "lint": "eslint src --ext .ts",
    "lintold": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "preview": "svelte-kit preview",
    "start": "node ./build/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/serve-static": "^2.2.2",
    "@typegoose/typegoose": "~9.4.0",
    "@yekalaitech/identity-security": "~0.0.1",
    "compression": "~1.7.4",
    "mongodb": "~4.2.2",
    "mongoose": "~6.1.4",
    "nestjs-typegoose": "~7.1.38",
    "oidc-provider": "~7.10.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.6",
    "@nestjs/schematics": "^8.0.5",
    "@sveltejs/kit": "next",
    "@types/express": "~4.17.13",
    "@types/oidc-provider": "~7.8.1",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "@yekalaitech/svelte-adapter-node": "0.0.1",
    "autoprefixer": "^10.4.1",
    "cssnano": "^5.0.14",
    "dotenv": "~10.0.0",
    "esbuild": "~0.14.10",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "svelte": "^3.44.3",
    "svelte-check": "^2.2.11",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  }
}
